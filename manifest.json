{
  "manifest_version": 3,
  "name": "Download Manager",
  "version": "1",
  "description": "Intercept Downloads of existing files",
  "permissions": [
    "downloads",
    "downloads.open",
    "storage"
  ],
  "background": {
    "service_worker": "bg.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/icon-16.png",
      "48": "images/icon-48.png",
      "128": "images/icon-128.png"
    }
  },
  "minimum_chrome_version": "117.0.5938.134",
  "icons": {
    "16": "images/icon-16.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  }
}
